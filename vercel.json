{
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        { "src": "/app.js", "dest": "/app.js" },
        { "src": "/about", "dest": "/about.html" },
        { "src": "/account", "dest": "/account.html" },
        { "src": "/home", "dest": "/home.html" },
        { "src": "/search", "dest": "/search.html" },
        { "src": "/(.*)", "dest": "/" }
    ]
}